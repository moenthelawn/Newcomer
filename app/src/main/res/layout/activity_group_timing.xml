            android:background="@color/white"
            android:textColor="@color/LightGrey"
            android:elevation="3dp"
            android:paddingLeft="2dp"
            android:padding="2dp"
            android:layout_height="wrap_content"
            android:id="@+id/prev2"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/your_state_progress_bar_id2"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="1.0"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginStart="1dp"
            android:layout_marginBottom="1dp"/>
    <TextView
            android:text="Next"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/LightGrey"
            android:background="@color/white"
            android:textStyle="bold"
            android:paddingLeft="4dp"
            android:padding="4dp"
            android:id="@+id/next2"
            android:elevation="3dp"
            android:textSize="20sp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/your_state_progress_bar_id2"
            app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginEnd="1dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" android:layout_marginBottom="1dp"/>
    <com.kofigyan.stateprogressbar.StateProgressBar
            android:id="@+id/your_state_progress_bar_id2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/white"

            android:elevation="3dp"
            app:spb_currentStateNumber="two"
            app:spb_maxStateNumber="four"
            app:spb_stateBackgroundColor="#BDBDBD"
            app:spb_stateForegroundColor="@color/DarkCyan"
            app:spb_stateNumberBackgroundColor="#808080"
            app:spb_stateNumberForegroundColor="#eeeeee"
            app:spb_currentStateDescriptionColor="@color/DarkCyan"
            app:spb_stateDescriptionColor="#808080"
            app:spb_animateToCurrentProgressState="true"
            app:spb_checkStateCompleted="true"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" app:layout_constraintHorizontal_bias="0.0"/>

    <LinearLayout
            android:orientation="horizontal"
            android:background="@color/LightGrey"
            android:divider="@drawable/horizontal_divider"
            android:layout_width="308dp"
            android:layout_height="2dp"
            android:focusable="true"
            android:id="@+id/linearLayout5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toTopOf="@+id/textView19"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.19">
    </LinearLayout>
    <EditText
            android:layout_width="133dp"
            android:layout_height="wrap_content"
            android:background="@drawable/underline"
            android:inputType="none"

            android:ems="10"
            android:id="@+id/editText4"
            android:windowSoftInputMode="stateHidden"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout5"
            app:layout_constraintTop_toBottomOf="@+id/textView15"/>
    <EditText
            android:layout_width="100dp"
            android:layout_height="32dp"
            android:background="@drawable/underline"
            android:inputType="time"
            android:windowSoftInputMode="stateHidden"
            android:ems="10"
            android:id="@+id/editText5"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout6"
            app:layout_constraintTop_toBottomOf="@+id/textView18" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <EditText
            android:layout_width="100dp"
            android:background="@drawable/underline"
            android:layout_height="32dp"
            android:inputType="number"
            android:windowSoftInputMode="stateHidden"
            android:ems="10"
            android:id="@+id/editText7"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toBottomOf="@+id/textView20"
            app:layout_constraintHorizontal_bias="0.503"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout7"
    />
    <TextView
            android:text="Choose the event date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView15" app:layout_constraintStart_toStartOf="@+id/editText4"
            app:layout_constraintEnd_toEndOf="@+id/editText4"
            android:textSize="22sp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp" app:layout_constraintBottom_toTopOf="@+id/linearLayout5"
            app:layout_constraintVertical_bias="0.0" android:textAppearance="@style/TextAppearance.AppCompat.Large"/>
    <TextView
            android:text="Choose the event start time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView18"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="@+id/editText5"
            app:layout_constraintEnd_toEndOf="@+id/editText5"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout5" app:layout_constraintVertical_bias="0.0"
            android:layout_marginTop="8dp" app:layout_constraintBottom_toTopOf="@+id/linearLayout6"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"/>
    <TextView
            android:text="Estimate the event end time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView20"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="@+id/editText7"
            app:layout_constraintEnd_toEndOf="@+id/editText7" app:layout_constraintTop_toBottomOf="@+id/linearLayout6"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout7" app:layout_constraintVertical_bias="0.0"
            android:layout_marginTop="8dp" android:textAppearance="@style/TextAppearance.AppCompat.Large"/>
    <TextView
            android:text="Choose the event age range"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView22"
            android:textSize="22sp"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout7"
            android:layout_marginTop="1dp"
            app:layout_constraintBottom_toTopOf="@+id/textView19" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintVertical_bias="0.0"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView19"
            app:layout_constraintStart_toEndOf="@+id/prev2"
            app:layout_constraintEnd_toStartOf="@+id/next2"
            app:layout_constraintBottom_toTopOf="@+id/your_state_progress_bar_id2"
            android:textSize="18sp"/>
    <TextView
            android:text="Min. Age"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView21"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintBottom_toBottomOf="@+id/rangeSeekbar5"
            app:layout_constraintTop_toTopOf="@+id/rangeSeekbar5" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <com.crystal.crystalrangeseekbar.widgets.CrystalRangeSeekbar
            android:id="@+id/rangeSeekbar5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:corner_radius="10"
            app:min_value="18"
            app:max_value="100"
            app:left_thumb_color="@color/DefaultCyan"
            app:right_thumb_color="@color/DefaultCyan"
            app:data_type="_integer" app:layout_constraintBottom_toTopOf="@+id/textView19"
            app:layout_constraintTop_toBottomOf="@+id/textView22"
            app:layout_constraintVertical_bias="0.26999998"
            tools:visibility="invisible"
            app:layout_constraintStart_toEndOf="@+id/textView21" app:layout_constraintEnd_toStartOf="@+id/textView27"/>
    <TextView
            android:text="1"
            android:gravity="center"
            android:textSize="18sp"
            android:background="@drawable/number"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:id="@+id/textView23"

            app:layout_constraintBottom_toTopOf="@+id/textView18" app:layout_constraintTop_toTopOf="@+id/textView15"
            app:layout_constraintEnd_toStartOf="@+id/editText4" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.0" app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginStart="2dp"/>
    <TextView
            android:text="2"
            android:gravity="center"
            android:textSize="18sp"
            android:background="@drawable/number"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:id="@+id/textView24"
            app:layout_constraintEnd_toStartOf="@+id/editText5"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toTopOf="@+id/linearLayout6"
            app:layout_constraintTop_toTopOf="@+id/textView18" app:layout_constraintVertical_bias="0.0"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginStart="2dp"/>
    <TextView
            android:text="3"
            android:gravity="center"
            android:textSize="18sp"
            android:background="@drawable/number"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:id="@+id/textView25"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout7"
            app:layout_constraintTop_toTopOf="@+id/textView20" app:layout_constraintEnd_toStartOf="@+id/editText7"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.0" android:layout_marginStart="2dp"/>
    <TextView
            android:text="4"
            android:gravity="center"
            android:textSize="18sp"
            android:background="@drawable/number"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:id="@+id/textView26"
            app:layout_constraintEnd_toStartOf="@+id/textView22"
            app:layout_constraintStart_toStartOf="parent"

            app:layout_constraintTop_toTopOf="@+id/textView22"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginStart="2dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>