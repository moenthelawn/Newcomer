            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintTop_toTopOf="@+id/rangeSeekbar5"
            app:layout_constraintBottom_toBottomOf="@+id/rangeSeekbar5" android:layout_marginEnd="8dp"/>
    <LinearLayout
            android:orientation="horizontal"
            android:background="@color/LightGrey"
            android:divider="@drawable/horizontal_divider"
            android:layout_width="308dp"
            android:layout_height="2dp"
            android:focusable="true"
            android:id="@+id/linearLayout6"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/textView19" app:layout_constraintVertical_bias="0.38">
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:background="@color/LightGrey"
            android:divider="@drawable/horizontal_divider"
            android:layout_width="308dp"
            android:layout_height="2dp"
            android:focusable="true"
            android:id="@+id/linearLayout7"
            app:layout_constraintBottom_toTopOf="@+id/textView19" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
