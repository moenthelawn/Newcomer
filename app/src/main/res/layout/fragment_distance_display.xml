<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".distanceDisplay" android:id="@+id/frameLayout">
    <ImageView
            android:src="@drawable/navigationframe"
            android:layout_width="0dp"
            android:layout_height="88dp"
            android:id="@+id/imageView2" android:scaleType="fitXY" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.00999999"
    />
    <TextView
            android:text="Distance"
            android:layout_width="94dp"
            android:layout_height="24dp"
            android:id="@+id/textView9"
            app:layout_constraintEnd_toEndOf="@+id/textView11"
            android:fontFamily="sans-serif"
            android:textSize="18sp" android:textAppearance="@style/TextAppearance.AppCompat.Large"
            app:layout_constraintStart_toStartOf="@+id/editText2"
            app:layout_constraintBottom_toBottomOf="@+id/editText2"
            app:layout_constraintTop_toTopOf="@+id/imageView2"
            app:layout_constraintVertical_bias="0.0" android:gravity="center"
            app:layout_constraintHorizontal_bias="0.51"/>
    <TextView
            android:text="km"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView11"
            android:fontFamily="sans-serif"
            android:textSize="18sp" android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:gravity="center"
            app:layout_constraintBottom_toBottomOf="@+id/editText2" app:layout_constraintTop_toTopOf="@+id/editText2"
            app:layout_constraintVertical_bias="0.58000004" app:layout_constraintStart_toEndOf="@+id/editText2"
    />
    <EditText
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editText2"
            app:layout_constraintEnd_toEndOf="@+id/imageView2"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="@+id/imageView2"
            app:layout_constraintTop_toTopOf="@+id/imageView2" app:layout_constraintVertical_bias="0.78"
            android:gravity="center"/>
</androidx.constraintlayout.widget.ConstraintLayout>